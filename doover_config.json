{
    "di_monitor": {
        "name": "di_monitor",
        "display_name": "DI Monitor",
        "type": "DEV",
        "visibility": "PUB",
        "allow_many": true,
        "description": "This app monitors a digital input. It shows the state of the DI and sends notifications when the alarm is triggered. It will also tell you the duration of the last triggered event.",
        "long_description": "README.md",
        "depends_on": [],
        "owner_org_key": "36a22e32-ab81-4a39-b400-869d532a052d",
        "image_name": "ghcr.io/getdoover/di-monitor:main",
        "container_registry_profile_key": "88953909-e9c4-4f81-9ec0-56239d4b0a2e",
        "build_args": "--platform linux/amd64,linux/arm64",
        "config_schema": {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "$id": "",
            "title": "Application Config",
            "type": "object",
            "properties": {
                "di_name": {
                    "title": "DI Name",
                    "x-name": "di_name",
                    "x-hidden": false,
                    "type": "string"
                },
                "di_channel": {
                    "title": "DI Channel",
                    "x-name": "di_channel",
                    "x-hidden": false,
                    "type": "integer",
                    "minimum": 0
                },
                "di_triggered_state": {
                    "enum": [
                        "rising",
                        "falling",
                        "ai_rising",
                        "ai_falling"
                    ],
                    "title": "DI Triggered State",
                    "x-name": "di_triggered_state",
                    "x-hidden": false,
                    "type": "string",
                    "default": "rising"
                },
                "alert_message": {
                    "title": "Alert Message",
                    "x-name": "alert_message",
                    "x-hidden": false,
                    "type": "string"
                },
                "alert_complete_message": {
                    "title": "Alert Complete Message",
                    "x-name": "alert_complete_message",
                    "x-hidden": false,
                    "type": "string"
                },
                "show_triggered_count": {
                    "title": "Show Triggered Count",
                    "x-name": "show_triggered_count",
                    "x-hidden": false,
                    "type": "boolean"
                }
            },
            "additionalElements": true,
            "required": [
                "di_name",
                "di_channel",
                "alert_message",
                "alert_complete_message",
                "show_triggered_count"
            ]
        },
        "key": "2a099e74-0af5-439e-a17b-f8d1bde1bbf5",
        "code_repo_key": null,
        "repo_branch": "main",
        "staging_config": {}
    }
}